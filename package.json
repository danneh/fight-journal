		"@fortawesome/vue-fontawesome": "^0.1.9",
		"@sweetalert2/theme-dark": "^3.1.2",
		"axios": "^0.21.1",
		"electron-reload": "^1.5.0",
		"electron-squirrel-startup": "^1.0.0",
