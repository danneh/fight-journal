{
	"name": "fight-journal",
	"productName": "Fight Journal",
	"author": "Daniel Hjellstr√∂m",
	"version": "1.1.0",
	"description": "Street Fighter V statistics",
	"repository": {
		"type": "git",
		"url": "https://github.com/danneh/fight-journal.git"
	},
	"main": "src/index.js",
	"scripts": {
		"start": "electron-forge start --inspect-electron NODE_ENV=development",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"lint": "echo \"No linting configured\"",
